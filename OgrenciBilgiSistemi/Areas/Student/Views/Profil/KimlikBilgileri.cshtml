@model EntityLayer.Concrete.Kullanicilar
@{
    ViewData["Title"] = "KimlikBilgileri";
    Layout = "~/Areas/Student/Views/StudentLayout.cshtml";
}

<div class="row">
    <div class="col-lg-12 stretch-card">
        <div class="card">
            <div class="card-body">
                <h4 class="card-title">Kimlik Bilgilerim</h4>
                <div class="table-responsive pt-3">
                    <table class="table table-bordered">
                        <thead>
                            <tr>
                                <th>
                                    #
                                </th>
                                <th>
                                    TC NO
                                </th>
                                <th>
                                    AD
                                </th>
                                <th>
                                    SOYAD
                                </th>
                                <th>
                                    DOĞUM YERİ
                                </th>
                                <th>
                                    DOĞUM TARİHİ
                                </th>
                            </tr>
                        </thead>
                        <tbody>
                            <tr class="table-info">
                                <td>
                                    1
                                </td>
                                <td>
                                    @Model.Kimlik.Tc
                                </td>
                                <td>
                                    @Model.Kimlik.Ad
                                </td>
                                <td>
                                    @Model.Kimlik.Soyad
                                </td>
                                <td>
                                    @Model.Kimlik.DogumYeri
                                </td>
                                <td>
                                    @Model.Kimlik.DogumTarihi
                                </td>
                            </tr>
                           
                        </tbody>
                    </table>
                </div>
            </div>
        </div>
    </div>
</div>