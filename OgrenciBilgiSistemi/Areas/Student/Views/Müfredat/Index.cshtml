@model EntityLayer.Concrete.Kullanicilar
@{
    ViewData["Title"] = "Index";
    Layout = "~/Areas/Student/Views/StudentLayout.cshtml";
}

<div class="row">
    <div class="col-lg-12 stretch-card">
        <div class="card">
            <div class="card-body">
                <h4 class="card-title">Müfredat Bilgilerim</h4>
                <div class="table-responsive pt-3">
                    <table class="table table-bordered">
                        <thead>
                            <tr>
                                <th>
                                    #
                                </th>
                                <th>
                                   Mufredat Adı
                                </th>
                               
                            </tr>
                        </thead>
                        <tbody>
                            <tr class="table-info">
                               
                                <td>
                                    @Model.Kimlik.Ogrenci.Mufredat.MufredatID
                                </td>
                                <td>
                                    @Model.Kimlik.Ogrenci.Mufredat.MufredatAdi
                                </td>
                               
                            </tr>

                        </tbody>
                    </table>
                </div>
            </div>
        </div>
    </div>
</div>

